{
  "name": "create-electron-secure-gatsby",
  "version": "1.0.0",
  "description": "Template for a project with Electron, Gatsby, Bytenode, Typescript, Asarmor",
  "main": "main.js",
  "author": "Stiliyan Kushev",
  "license": "MIT",
  "private": false,
  "build": {
    "icon": "src/images/icon.png",
    "productName": "example",
    "files": [
      "public/**/*",
      "scripts/*.js",
      "scripts/*.jsc",
      "main.js",
      "main-src.js"
    ],
    "win": {},
    "linux": {},
    "mac": {},
    "asar": false
  },
  "scripts": {
    "build": "npx tsc --project ./tsconfig.json",
    "develop": "gatsby develop",
    "compile-main": "node compile-main.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prebuild": "npm run clean",
    "electron": "wait-on http://localhost:8000 && electron .",
    "electron-pack": "electron-builder",
    "dev": "node predev.js && npm run build && concurrently \"npm run develop\" \"npm run electron\"",
    "pack": "node predev.js && npm run build && gatsby build && npm run electron-pack && npm run compile-main"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@herberttn/bytenode-webpack-plugin": "^1.2.5",
    "asarmor": "^1.8.0",
    "kill-process-by-name": "^1.0.5",
    "electron-serve": "^1.1.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "^16.7.13",
    "concurrently": "^5.3.0",
    "electron": "^9.2.1",
    "electron-builder": "^22.8.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^1.5.0",
    "electron-serve": "^1.1.0",
    "electron-util": "^0.16.0",
    "fs": "0.0.1-security",
    "gatsby": "^2.24.47",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-manifest": "^2.4.23",
    "gatsby-plugin-offline": "^3.2.23",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-sharp": "^2.5.13",
    "prettier": "2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "wait-on": "^5.2.0"
  },
  "keywords": [
    "cross-platform",
    "native-app",
    "installed-app",
    "desktop-applications",
    "windows",
    "linux",
    "macos",
    "html",
    "css",
    "javascript",
    "front-end",
    "blog",
    "gatsby",
    "electron",
    "electron-builder",
    "electron-serve",
    "concurrently",
    "wait-on",
    "react",
    "static-site-generator"
  ]
}
